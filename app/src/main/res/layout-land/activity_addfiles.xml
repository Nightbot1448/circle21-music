<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/file"
        android:layout_width="199dp"
        android:layout_height="32dp"
        android:layout_marginStart="321dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="250dp"
        android:text="Adding files"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/official"
        android:layout_width="112dp"
        android:layout_height="57dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="20dp"
        android:text="Offical library"
        app:layout_constraintEnd_toEndOf="@+id/from"
        app:layout_constraintTop_toBottomOf="@+id/from" />

    <Button
        android:id="@+id/my"
        android:layout_width="126dp"
        android:layout_height="56dp"
        android:layout_marginStart="10dp"
        android:text="My library"
        app:layout_constraintStart_toEndOf="@+id/official"
        app:layout_constraintTop_toTopOf="@+id/official" />

    <TextView
        android:id="@+id/from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="150dp"
        android:text="FROM"
        android:textSize="15dp"
        app:layout_constraintEnd_toStartOf="@+id/file"
        app:layout_constraintTop_toBottomOf="@+id/file" />

    <TextView
        android:id="@+id/to"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="8dp"
        android:text="TO"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/file"
        app:layout_constraintTop_toBottomOf="@+id/file" />

    <TextView
        android:id="@+id/ex1b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="3dp"
        android:text="Kicks"
        android:textSize="15dp"
        app:layout_constraintEnd_toStartOf="@+id/to"
        app:layout_constraintTop_toBottomOf="@+id/to" />

    <TextView
        android:id="@+id/ex2b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Snares"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/ex1b"
        app:layout_constraintTop_toBottomOf="@+id/ex1b" />

    <TextView
        android:id="@+id/ex3b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Hihats"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/ex2b"
        app:layout_constraintTop_toBottomOf="@+id/ex2b" />

    <TextView
        android:id="@+id/ex4b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="New"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/ex3b"
        app:layout_constraintTop_toBottomOf="@+id/ex3b" />

    <TextView
        android:id="@+id/ex1a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:text="Kick 1"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/from"
        app:layout_constraintTop_toBottomOf="@+id/from" />

    <TextView
        android:id="@+id/ex2a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Kick 2"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/ex1a"
        app:layout_constraintTop_toBottomOf="@+id/ex1a" />

    <TextView
        android:id="@+id/ex3a"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Kick 3"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/ex2a"
        app:layout_constraintTop_toBottomOf="@+id/ex2a" />

    <TextView
        android:id="@+id/find"
        android:layout_width="111dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:text="find something"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="@+id/ex1a"
        app:layout_constraintTop_toBottomOf="@+id/ex3a" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="110dp"
        android:onClick="openFile"
        android:clickable="true"
        android:contentDescription="Find a file"
        app:layout_constraintStart_toEndOf="@+id/find"
        app:layout_constraintTop_toBottomOf="@+id/my"
        app:srcCompat="@android:drawable/ic_menu_search" />
</androidx.constraintlayout.widget.ConstraintLayout>